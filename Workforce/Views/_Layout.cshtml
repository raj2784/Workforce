                <!--- Sidemenu -->
                <div id="sidebar-menu">
                    <!-- Left Menu Start -->
                    <ul class="metismenu" id="side-menu">
                        <li class="menu-title">Menu</li>
                        <li>@Html.ActionLink("Home", "Index", "Home")</li>
                        <li>@Html.ActionLink("About Us", "About", "Home")</li>


                        @{
                            if (HttpContext.Current.User.Identity.IsAuthenticated)
                            {
                                <li>
                                    <a href="javascript: void(0);">
                                        <span class="badge badge-success float-right"></span> <span>Employee</span>
                                    </a>
                                    <ul class="nav-second-level" aria-expanded=false>
                                        <li>@Html.ActionLink("Add Employee", "Addemployee", "Home")</li>
                                        <li>@Html.ActionLink("Employee List", "Employeelist", "Home")</li>
                                    </ul>
                                </li>
                            }
                        }
                        <li>
                            <a href="javascript: void(0);">
                                <span class="badge badge-success float-right"></span> <span> Account</span>
                            </a>
                            <ul class="nav-second-level" aria-expanded=false>
                                <li>@Html.ActionLink("Loggin", "Emploggin", "Home")</li>

                                @{
                                    if (HttpContext.Current.User.Identity.IsAuthenticated)
                                    {

                                        <li>@Html.ActionLink("Change Password", "Changepassword", "Home")</li>
                                        <li>@Html.ActionLink("LogOut", "Logout", "Home")</li>

                                    }
                                }

                            </ul>
                        </li>
                        <li>@Html.ActionLink("Contact Us", "Contactus", "Home")</li>

                    </ul>

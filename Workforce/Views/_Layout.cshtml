        <!-- Left Sidebar End -->
        <!-- ============================================================== -->
        <!-- Start right Content here -->
        <!-- ============================================================== -->
        <div class="content-page">
            <!-- Start content -->
            <div class="content">
                @RenderBody() <!-- container -->

            </div> <!-- content -->

            <footer class="footer">
                2010- 2019 © Worforce Infotech Pty Ltd.
            </footer>
        </div>

